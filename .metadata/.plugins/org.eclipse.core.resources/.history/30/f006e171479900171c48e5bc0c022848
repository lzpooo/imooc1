/**
 * 服务器端
 */
public class ServerMain {
	public static void main(String[] args) {
		System.out.println("这是服务器：  ");
		try {
			new Configuration().getServer().revicer();
			System.out.println("===================================");
			new Configuration().getBackup().store("1", new GatherImpl().getMap(), true);
		} catch (Exception e) {
			e.printStackTrace();
		}
	}
}
