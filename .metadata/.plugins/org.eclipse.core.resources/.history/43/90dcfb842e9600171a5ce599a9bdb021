package com.briup.mytest.pojo;

/**
 * 妻子类
 * 
 * @author LZP 
 * 
 * 2017年9月10日
 */
public class Wife {
	private String name;
	private int age;
	private int id;
	private Husband husband;

	public Wife(String name, int age, int id, Husband husband) {
		this.name = name;
		this.age = age;
		this.id = id;
		this.husband = husband;
	}

	public Wife(String name, int age, Husband husband) {
		this.name = name;
		this.age = age;
		this.husband = husband;
	}

	public Wife() {
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public Husband getHusband() {
		return husband;
	}

	public void setHusband(Husband husband) {
		this.husband = husband;
	}

	@Override
	public String toString() {
		return "Wife [name=" + name + ", age=" + age + ", id=" + id
				+ ", husband=" + husband + "]";
	}

}
