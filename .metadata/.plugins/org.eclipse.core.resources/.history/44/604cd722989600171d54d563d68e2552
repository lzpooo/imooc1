package com.briup.mytest.pojo;

import java.util.List;

/**
 * 
 * @author LZP
 * 2017年9月11日
 */
public class Student {
	private Integer id;
	private String name;
	private String gender;
	private Integer age;
	private List<Teacher> listTeacher;
	public Student(Integer id, String name, String gender, Integer age,
			List<Teacher> listTeacher) {
		this.id = id;
		this.name = name;
		this.gender = gender;
		this.age = age;
		this.listTeacher = listTeacher;
	}
	public Student(String name, String gender, Integer age,
			List<Teacher> listTeacher) {
		this.name = name;
		this.gender = gender;
		this.age = age;
		this.listTeacher = listTeacher;
	}
	public Student() {
	}
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getGender() {
		return gender;
	}
	public void setGender(String gender) {
		this.gender = gender;
	}
	public Integer getAge() {
		return age;
	}
	public void setAge(Integer age) {
		this.age = age;
	}
	public List<Teacher> getListTeacher() {
		return listTeacher;
	}
	public void setListTeacher(List<Teacher> listTeacher) {
		this.listTeacher = listTeacher;
	}
	@Override
	public String toString() {
		return "Student [id=" + id + ", name=" + name + ", gender=" + gender
				+ ", age=" + age + ", listTeacher=" + listTeacher + "]";
	} 
	
}
