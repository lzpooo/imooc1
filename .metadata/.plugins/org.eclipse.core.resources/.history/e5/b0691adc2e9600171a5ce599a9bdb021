package com.briup.mytest.pojo;

/**
 * 丈夫类
 * 
 * @author LZP 
 * 
 * 2017年9月10日
 */
public class Husband {
	private int id;
	private String name;
	private int age;

	public Husband(int id, String name, int age) {
		this.id = id;
		this.name = name;
		this.age = age;
	}

	public Husband() {
	}

	public Husband(String name, int age) {
		this.name = name;
		this.age = age;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}

	@Override
	public String toString() {
		return "Husband [id=" + id + ", name=" + name + ", age=" + age + "]";
	}

}
